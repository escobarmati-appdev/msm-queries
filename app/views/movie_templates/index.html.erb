<h1>
  List of all movies
</h1>

<table border="1">
<% @list_of_movies.each do |a_thing|  %>
<tr>
  <td> Id <%#= a_thing.id %> </td>
  <td> 
    Image <img src="<%= a_thing.image %>">"
  </td>
  <td> Year <%#= a_thing.name %> </td>
  <td> Director <%#= a_thing.dob %> </td>
  <td> 
    <a href="/movies/<%= a_thing.id %>">
    Show details
    </a>
  </td>
</tr>
<% end %>
</table>
